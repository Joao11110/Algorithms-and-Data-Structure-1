#include<stdio.h>

int soma (int a, int b);

int main(){
    int num1, num2, s;
    printf("Primeiro numeros: ");
    scanf("%d", &num1);
    printf("\nSegundo numero: ");
    scanf("%d",&num2);
    s=soma(num1, num2);
    printf("\nSoma: %d\n", s);
    return 0;
}

int soma (int a, int b){
    int i, s=0;
    for(i = a; i <= b; i++){
        s=s+i;
    }
    return(s);
}
